<mvc:View 
  id="ComprobanteView"  
  controllerName="zetpmprorate.controller.Worklist"  
  xmlns="sap.m" 
  xmlns:mvc="sap.ui.core.mvc"
  xmlns:f="sap.ui.layout.form"
  xmlns:core="sap.ui.core" 
  xmlns:m="sap.m" 
  xmlns:l="sap.ui.layout" >  
  <f:SimpleForm		
		id="ComprobanteForm"
		columnsL="3"
		columnsM="3"
		editable="true"
		layout="ResponsiveGridLayout">		
		<f:content>	
        <Table
                id="tableComprobante"
                width="auto"
                items="{
                    path: 'ProrationModel>/ExpenseSet',
                    sorter: {
                        path: 'ExpedienteId',
                        descending: false
                    }
                }"
                noDataText="{worklistView>/tableNoDataText}"
                busyIndicatorDelay="{worklistView>/tableBusyDelay}"
                growing="true"
                growingScrollToLoad="true"
                updateFinished=".onUpdateFinished2">

                <headerToolbar>
                    <OverflowToolbar>
                        <Title
                            id="tableHeader"
                            text="{worklistView>/worklistTableTitle2}"
                            level="H3"/>
                        <ToolbarSpacer />
                        <SearchField
                            id="searchField"
                            tooltip="{i18n>worklistSearchTooltip}"
                            search=".onSearch">
                            <layoutData>
                                <OverflowToolbarLayoutData
                                    maxWidth="200px"
                                    priority="NeverOverflow"/>
                            </layoutData>
                        </SearchField>
                    </OverflowToolbar>
                </headerToolbar>

                <columns>                    
                    <Column id="ExpedienteName">
                        <Text text="{i18n>tableExpedienteNameTitle}" id="ExpedienteNameTitle"/>
                    </Column>                    
                    <Column id="Cuenta">
                        <Text text="{i18n>tableCuentaTitle}" id="CuentaTitle"/>
                    </Column>
                    <Column id="Gasto">
                        <Text text="{i18n>tableGastoTitle}" id="GastoTitle"/>
                    </Column>                                        
                </columns>

                <items>
                    <ColumnListItem id="_IDGenColumnListItem1" >  
                      <customData>
                      <core:CustomData key="mygasto" value="{ProrationModel>Cuenta}" writeToDom="true" >
                      </core:CustomData>
                     </customData>

                        <Text id="_IDGenText2" text="{ProrationModel>ExpedienteName}"/>                        
                        <Text id="_IDGenText3" text="{ProrationModel>Cuenta}"/>
                        <Text id="_IDGenText4" text="{ProrationModel>Gasto}"/>                        
                    </ColumnListItem>
                </items>
            </Table>        
        </f:content>
    </f:SimpleForm>	
</mvc:View> 