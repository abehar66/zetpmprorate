<mvc:View 
  id="TraspasoView"  
  controllerName="zetpmprorate.controller.Worklist"  
  xmlns="sap.m" 
  xmlns:mvc="sap.ui.core.mvc"
  xmlns:f="sap.ui.layout.form"
  xmlns:core="sap.ui.core" 
  xmlns:m="sap.m" 
  xmlns:l="sap.ui.layout" >  
  <f:SimpleForm		
		id="TraspasoForm"
		columnsL="3"
		columnsM="3"
		editable="true"
		layout="ResponsiveGridLayout">		
		<f:content>	        
         <Table
                id="tableTraspaso"
                width="auto"
                items="{
                    path: 'ProrationModel>/TraspasoSet'
                    
                }"
                noDataText="{worklistView>/tableNoDataText}"
                busyIndicatorDelay="{worklistView>/tableBusyDelay}"
                growing="true"
                growingScrollToLoad="true"
                updateFinished=".onUpdateFinished">

                <headerToolbar>
                    <OverflowToolbar>
                        <Title
                            id="tableHeaderTraspaso"
                            text="{worklistView>/worklistTableTitle}"
                            level="H3"/>
                        <ToolbarSpacer />
                        <SearchField
                            id="searchFieldTranspaso"
                            tooltip="{i18n>worklistSearchTooltip}"
                            search=".onSearch">
                            <layoutData>
                                <OverflowToolbarLayoutData
                                    maxWidth="200px"
                                    priority="NeverOverflow"/>
                            </layoutData>
                        </SearchField>
                    </OverflowToolbar>
                </headerToolbar>

                <columns>                   
                    <Column id="ExpedienteNameTrapaso">
                        <Text text="{i18n>tableExpedienteNameTitle}" id="ExpedienteNameTitleTraspaso"/>
                    </Column>
                     <Column id="Centro">
                        <Text text="{i18n>tableCentroTitle}" id="CentroTitle"/>
                    </Column> 
                    <Column id="CentroTel">
                        <Text text="{i18n>tableCentroTelTitle}" id="CentroTelTitle"/>
                    </Column> 
                    <Column id="PiezaTraspaso">
                        <Text text="{i18n>tablePiezaTitle}" id="PiezaTitleTraspaso"/>
                    </Column> 
                    <Column id="PiezaDescTraspaso">
                        <Text text="{i18n>tablePiezaDescTitle}" id="PiezaDescTitleTraspaso"/>
                    </Column>        
                     <Column id="ModeloTraspaso">
                        <Text text="{i18n>tableModeloTitle}" id="ModeloTitleTraspaso"/>
                    </Column>                    
                    <Column id="CantTraspaso" hAlign="Center">
                        <Text text="{i18n>tableCantTitle}" id="CantTitleTraspaso"/>
                    </Column>                                    
                </columns>

                <items>
                    <ColumnListItem id="_IDGenColumnListItemTraspaso"  >
                    <customData>
                    <core:CustomData key="mytraspaso" value="{ProrationModel>Centro}" writeToDom="true" >
                    </core:CustomData>
                    </customData>
                                               
                        <Text id="_IDGenTextTraspaso1" text="{ProrationModel>ExpedienteName}"/>
                        <Text id="_IDGenTextTraspaso2" text="{ProrationModel>Centro}"/>
                        <Text id="_IDGenTextTraspaso3" text="{ProrationModel>Ctel}"/>
                        <Text id="_IDGenTextTraspaso4" text="{ProrationModel>Pieza}"/>
                        <Text id="_IDGenTextTraspaso5" text="{ProrationModel>PiezaDesc}"/>
                        <Text id="_IDGenTextTraspaso6" text="{ProrationModel>Modelo}"/>  
                        <ObjectNumber number="{
								path: 'ProrationModel>Cant'                                
							}" />                                                                      
                    </ColumnListItem>
                </items>
            </Table>
        </f:content>
    </f:SimpleForm>	
</mvc:View>  